# Supabase Configuration
# Get these values from Supabase Dashboard > Settings > API
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=

# Supabase Database Password (for schema management)
SUPABASE_DB_PASSWORD=

# MongoDB Atlas Data API Configuration (deprecated - keeping for reference)
# Get these values from MongoDB Atlas Dashboard > Data API
VITE_MONGODB_DATA_API_URL=
VITE_MONGODB_DATA_API_KEY=
VITE_MONGODB_DATABASE=deskplanner
VITE_MONGODB_CLUSTER=Cluster0

# Storage type: 'localStorage', 'mongodb', 'supabase', or 'hybrid'
# hybrid = localStorage with Supabase sync (recommended for reliability)
VITE_STORAGE_TYPE=hybrid